{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es2016",
    "esModuleInterop": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "paths": {
      "#*": ["*"],
    }
  }
}
